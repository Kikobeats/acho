{
  "name": "acho",
  "description": "An extremely simple (but powerful) logging system for NodeJS and browser.",
  "homepage": "https://github.com/Kikobeats/acho",
  "version": "2.4.1",
  "main": "./index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": "Kikobeats/acho",
  "bugs": {
    "url": "https://github.com/Kikobeats/acho/issues"
  },
  "keywords": [
    "log",
    "logger",
    "logging"
  ],
  "dependencies": {
    "chalk": "latest",
    "coffee-script": "latest",
    "existential-assign": "latest",
    "format-util": "latest",
    "pretty-ms": "latest"
  },
  "devDependencies": {
    "browserify": "latest",
    "coffeeify": "latest",
    "date-utils": "latest",
    "git-dirty": "latest",
    "gulp": "latest",
    "gulp-header": "latest",
    "gulp-uglify": "latest",
    "gulp-util": "latest",
    "mocha": "latest",
    "set-dateout": "latest",
    "should": "latest",
    "standard": "latest",
    "titleize": "latest",
    "vinyl-buffer": "latest",
    "vinyl-source-stream": "latest"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "dirty": "./node_modules/.bin/git-dirty",
    "commit": "git add CHANGELOG.md bower.json package.json dist && git commit -m \"$newVersion releases\"",
    "lint": "./node_modules/.bin/standard lib",
    "prepublish": "npm run dirty && npm run clean && npm install && npm test",
    "pretest": "npm run lint",
    "push": "git tag $newVersion && git push origin $newVersion --follow-tags",
    "test": " && sh test/test.sh"
  },
  "license": "MIT"
}
